<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Cura Healthcare" verbose="2" 
parallel="tests" thread-count="2">

	<test name="Login tests">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.curahealthecareTestNG.Tests.login.LoginTests"></class>
		</classes>
	</test>
	
	<test name="Make Appointment tests">
	<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.curahealthecareTestNG.Test.MakeAppoitment.MakeAppointmentTest"></class>
		</classes>
	</test>

</suite>
